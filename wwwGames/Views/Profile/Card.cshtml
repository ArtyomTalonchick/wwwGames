@model IEnumerable<wwwGames.Models.User>

<table>
    <tr>
        <td>Имя</td>
        <td>Мыло</td>
        <td></td>
    </tr>
    @foreach (var user in Model)
    {
    <tr>
        <td>@user.Name</td>
        <td>@user.Email</td>
        <a asp-controller="Profile" asp-action="Card" asp-route-id="@user.Name"></a>
    </tr>
    }
</table>